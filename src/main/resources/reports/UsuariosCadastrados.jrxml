	<queryString>
		<![CDATA[select distinct u.id_usuario, u.nome, u.login, u.situacao, u.dt_cadastro,
p.id_perfil, coalesce(p.descricao, '-') as descricao_perfil
from usuario u
left join usuario_perfil up on up.id_usuario = u.id_usuario
